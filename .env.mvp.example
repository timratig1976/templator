# MVP Database Environment Variables
# Copy this file to .env and update the values

# PostgreSQL Configuration
POSTGRES_DB=templator
POSTGRES_USER=templator_user
POSTGRES_PASSWORD=your_secure_password_here
POSTGRES_PORT=5432

# Database URL for Prisma (update with your actual password)
DATABASE_URL="postgresql://templator_user:your_secure_password_here@localhost:5432/templator?schema=public"
